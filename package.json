    "axios": "^0.18.0",
    "bcrypt": "^5.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-session": "^1.17.1",
    "multer": "^1.4.2",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.1",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "password-generator": "^2.3.2",
    "path": "^0.12.7",
    "sequelize": "^4.39.0",
    "twilio": "^3.50.0",
    "underscore": "^1.11.0"
  }
