  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "proxy": "http://localhost:3001",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "express": "^4.16.3",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.1",
    "sequelize": "^4.39.0"
  }
}
